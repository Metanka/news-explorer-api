{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "import/no-unresolved": [
        2, 
        { "caseSensitive": false }
     ]
    }
}
